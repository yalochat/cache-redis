    "hoek": "^6.0.3"
  },
  "devDependencies": {
    "code": "^5.2.3",
    "dotenv": "^6.1.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-watch": "^5.0.1",
    "husky": "^1.1.3",
    "lab": "^17.3.0",
    "lint-staged": "^8.0.4",
